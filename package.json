  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      },
      "default": {
        "types": "./dist/umd/types/index.d.ts",
        "default": "./dist/umd/index.js"
      }
    }
